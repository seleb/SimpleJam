pico-8 cartridge // http://www.pico-8.com
version 7
__lua__
cam = {}
player = {}


function lerp(a,b,t)
 return a+(b-a)*t
end

function _init()
 test = 0
 
 player.x = 0
 player.y = 0
 player.a = 0
 player.vx = 0
 player.vy = 0
 player.va = 0
 
 cam.x = player.x
 cam.y = player.y
end

function _update()
 local speed = 5
 if btn(0) then player.vx -= speed end
 if btn(1) then player.vx += speed end
 if btn(2) then player.vy -= speed end
 if btn(3) then player.vy += speed end

 if btn(4) then player.va -= speed/3 end
 if btn(5) then player.va += speed/3 end



	player.vx = lerp(player.vx,0,0.8)
	player.vy = lerp(player.vy,0,0.8)
 player.va = lerp(player.va,0,0.8)
 
 player.x += player.vx
 player.y += player.vy
 player.a += player.va
 
 cam.x = lerp(cam.x, player.x-64, 0.25)
 cam.y = lerp(cam.y, player.y-64, 0.25)
end

function _draw()
 camera(0,0)
 color(2)
 rectfill(0,0,128,128)
 
 camera(cam.x, cam.y)
 
 draw_boat()
 
 draw_waves()
 --[[color(2)
 for x = cam.x,cam.x+128 do
  line(x,11+wave(x),x,12+wave(x+1))
 end]]
end

function draw_boat()
 color(1)
 circ(player.x,player.y,10)
 line(player.x,player.y,player.x + cos(player.a)*20, player.y + sin(player.a)*20)
end

function draw_waves()
 color(1)
 for x = cam.x,cam.x+128 do
  local w1 = wave(x)
  local w2 = wave(x+1)
  line(x,10+w1,x,max(11+w1,10+w2))
 end
 color(2)
 for x = cam.x,cam.x+128 do
  local w1 = wave(x)
  local w2 = wave(x+1)
  line(x,12+w1,x,max(13+w1,12+w2))
 end
end

function wave(x)
 local w1 = sin((x+time()*3)/50)
 return flr(abs(w1)*5)
end
